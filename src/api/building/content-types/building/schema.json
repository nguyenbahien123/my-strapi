{
  "kind": "collectionType",
  "collectionName": "buildings",
  "info": {
    "singularName": "building",
    "pluralName": "buildings",
    "displayName": "Building"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "BuildingName": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Address": {
      "type": "text",
      "required": true
    },
    "NumberOfFloors": {
      "type": "integer",
      "required": true
    },
    "YearBuilt": {
      "type": "integer"
    },
    "Description": {
      "type": "text"
    },
    "vote_surveys": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::vote-survey.vote-survey",
      "mappedBy": "buildings"
    },
    "service_fees": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::service-fee.service-fee",
      "mappedBy": "buildings"
    }
  }
}
