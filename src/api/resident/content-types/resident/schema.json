{
  "kind": "collectionType",
  "collectionName": "residents",
  "info": {
    "singularName": "resident",
    "pluralName": "residents",
    "displayName": "Resident"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "phoneNumber": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "avatar": {
      "type": "string"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "statuscode": {
      "type": "enumeration",
      "default": "Active",
      "enum": [
        "Active",
        "Pending_Accept"
      ]
    },
    "gender": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::gender.gender"
    },
    "buildings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::building.building"
    },
    "zaloId": {
      "type": "text",
      "unique": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "payments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::payment.payment",
      "mappedBy": "payer"
    },
    "family": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::family.family"
    },
    "survey_responses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::survey-response.survey-response",
      "mappedBy": "resident"
    },
    "service_registrations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service-registration.service-registration",
      "mappedBy": "resident"
    },
    "payment_histories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::payment-history.payment-history",
      "mappedBy": "resident"
    }
  }
}
