{
  "kind": "collectionType",
  "collectionName": "feedbacks",
  "info": {
    "singularName": "feedback",
    "pluralName": "feedbacks",
    "displayName": "Feedback"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Type": {
      "type": "enumeration",
      "enum": [
        "Suggest",
        "Issue",
        "Question"
      ]
    },
    "Title": {
      "type": "text"
    },
    "Content": {
      "type": "richtext"
    },
    "attachments": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Resident": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::resident.resident"
    },
    "StatusFeedback": {
      "type": "enumeration",
      "default": "Chưa xử lý",
      "enum": [
        "Chưa xử lý",
        "Đang xử lý",
        "Đã xử lý"
      ]
    }
  }
}
