{
  "kind": "collectionType",
  "collectionName": "questions",
  "info": {
    "singularName": "question",
    "pluralName": "questions",
    "displayName": "Question"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "content": {
      "type": "text",
      "required": true,
      "unique": false
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "single_choice",
        "multiple_choice",
        "text"
      ]
    },
    "vote_survey": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::vote-survey.vote-survey",
      "inversedBy": "questions"
    },
    "options": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::option.option",
      "mappedBy": "question"
    },
    "survey_answers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::survey-answer.survey-answer",
      "mappedBy": "question"
    }
  }
}
